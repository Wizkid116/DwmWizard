#Define which software versions to use
DWMVER=dwm-6.5
DMUVER=dmenu-5.3
SLSVER=slstatus-1.0

#Define source and build directories
SRC=~/DwmWizard
BLD=$SRC/build
DWM=$BLD/$DWMVER
SLS=$BLD/$SLSVER
DMU=$BLD/$DMUVER

#Define download links
DL1=https://dl.suckless.org/dwm/$DWMVER.tar.gz
DL2=https://dl.suckless.org/tools/$DMUVER.tar.gz
DL3=https://dl.suckless.org/tools/$SLSVER.tar.gz
